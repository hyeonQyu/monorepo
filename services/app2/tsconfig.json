{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "src",
    "paths": {
      "@pages/*": [
        "./pages/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@defines/*": [
        "./defines/*"
      ],
      "@icons/*": [
        "./icons/*"
      ],
      "@apis/*": [
        "./apis/*"
      ],
      "@stores/*": [
        "./stores/*"
      ],
      "@i18n/*": [
        "./i18n/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@public/*": [
        "../public/*"
      ],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
